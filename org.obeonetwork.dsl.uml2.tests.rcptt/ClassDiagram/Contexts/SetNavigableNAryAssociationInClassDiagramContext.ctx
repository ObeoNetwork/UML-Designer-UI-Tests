        with [get-target-connection 2] {
            get-edit-part -className "" -index 1 | get-property "getLabelText()" | equals "class3s[*]" | verify-true
        }
    }
}
with [get-editor "test Class Diagram"] {
    with [get-diagram -index 1] {
        key-type ESC -times 7
    }
}
